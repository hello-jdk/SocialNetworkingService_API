const app = require("../app");
const { SERVER } = require("../config");

app
  .listen(SERVER.PORT, () => {
    console.log(`
      ################################################
        ðŸ›¡ï¸  Server listening on port: ${SERVER.PORT} ðŸ›¡ï¸
      ################################################`);
  })
  .on("error", (err) => {
